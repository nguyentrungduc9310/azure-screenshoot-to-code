{
  "name": "@screenshot-to-code/data-layer",
  "version": "1.0.0",
  "description": "Data layer services for Screenshot to Code application",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:create": "ts-node src/scripts/create-database.ts",
    "db:migrate": "ts-node src/scripts/migrate-database.ts",
    "db:seed": "ts-node src/scripts/seed-database.ts",
    "db:reset": "ts-node src/scripts/reset-database.ts",
    "cache:flush": "ts-node src/scripts/flush-cache.ts",
    "storage:init": "ts-node src/scripts/init-storage.ts"
  },
  "keywords": [
    "postgresql",
    "redis",
    "azure-blob",
    "typeorm",
    "caching",
    "storage"
  ],
  "author": "Screenshot to Code Team",
  "license": "MIT",
  "dependencies": {
    "typeorm": "^0.3.17",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "@azure/storage-blob": "^12.17.0",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4",
    "uuid": "^9.0.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@types/pg": "^8.10.7",
    "@types/uuid": "^9.0.6",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/jest": "^29.5.6",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}